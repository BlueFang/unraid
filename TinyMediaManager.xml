<?xml version="1.0"?>
<Container>
  <Name>TinyMediaManager</Name>
  <Repository>romancin/tinymediamanager</Repository>
  <Registry>https://hub.docker.com/r/romancin/tinymediamanager</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/romancin/tinymediamanager-docker</Support>
  <Project></Project>
  <Overview>TinyMediaManager</Overview>
  <Description>使用romancin的TinyMediaManager镜像，添加了中文支持和密码支持。</Description>
  <Category>Cloud: Network:Web Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:5800]/</WebUI>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
  <Data>
    <Volume>
      <HostDir>Input your MEDIA folder!</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>Input your CONFIG folder</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Name>ENABLE_CJK_FONT</Name>
      <Value>1</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>VNC_PASSWORD</Name>
      <Value>Input your password!</Value>
      <Mode/>
    </Variable>
  </Environment>
  <Networking>
      <Port>
        <HostPort>5800</HostPort>
        <ContainerPort>5800</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5900</HostPort>
        <ContainerPort>5900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
  </Networking>
</Container>